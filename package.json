{
  "name": "@aditzel/caldotcom-api-v2-sdk",
  "version": "0.2.0",
  "description": "TypeScript SDK for Cal.com v2 API - Modern, type-safe client for interacting with Cal.com's scheduling platform",
  "author": {
    "name": "Allan Ditzel",
    "url": "https://github.com/aditzel"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aditzel/caldotcom-api-v2-sdk.git"
  },
  "keywords": [
    "cal.com",
    "caldotcom",
    "calendar",
    "scheduling",
    "booking",
    "appointments",
    "api",
    "sdk",
    "typescript",
    "cal-api"
  ],
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "license": "MIT",
  "packageManager": "bun@1.3.4",
  "engines": {
    "bun": "^1.3.4",
    "node": ">=18"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "bash ./scripts/build.sh",
    "dev": "bunx concurrently -n esm,cjs,types \"bun build ./src/index.ts --format esm --outfile ./dist/index.mjs --watch\" \"bun build ./src/index.ts --format cjs --outfile ./dist/index.cjs --watch\" \"bunx tsc -p tsconfig.build.json -w\"",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "typecheck": "bunx tsc -p tsconfig.json --noEmit"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/bun": "^1.3.4",
    "@types/node": "^24.10.2",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0"
  }
}
